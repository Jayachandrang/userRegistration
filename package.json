    "babel-preset-react": "^6.24.1",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "env": "0.0.2",
    "express": "^4.16.4",
    "gh-pages": "^3.0.0",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.2.4",
    "multer": "^1.3.1",
    "nodemon": "^1.18.3"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^3.6.0",
